{
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "indent": [
      2,
      2,
    ],
    "no-func-assign": 0,
    "no-constant-condition": 0,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2,
        "maxEOF": 1
      }
    ],
    "curly": [
      2,
      "multi",
      "consistent"
    ],
    "eol-last": [
      2,
      "unix"
    ]
  },
  "globals": {
    describe: false,
    beforeEach: false,
    beforeAll: false,
    afterEach: false,
    afterAll: false,
    jasmine: false,
    spyOn: false,
    it: false,
    expect: false
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}

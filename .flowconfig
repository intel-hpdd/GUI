[ignore]
.*node_modules/babel-.*
.*node_modules/babylon.*
.*node_modules/intel-socket-worker.*
.*intel-fp/test/fp-test.js
.*node_modules/highland.*
.*\(lib\|test\|builtin-modules\|binary-extensions\|faker\|spdx\).*\.json

[include]
node_modules/intel-flow-highland/include/

[libs]
node_modules/intel-flow-highland/libs/highland.js

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
module.name_mapper='^intel-parsely/\(.*\)\.js$' -> '<PROJECT_ROOT>/node_modules/intel-parsely/dist/source/\1.js.flow'
module.name_mapper='^intel-qs-parsers/\(.*\)\.js$' -> '<PROJECT_ROOT>/node_modules/intel-qs-parsers/dist/source/\1.js.flow'
esproposal.class_instance_fields=enable
